{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Error",
        "System": "Error"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId", "WithHttpRequest" ]
  },
  "ManagedIdentityClientId": "00000000-0000-0000-0000-000000000000",
  "ApplicationInsights": {
    "InstrumentationKey": "00000000-0000-0000-0000-000000000000"
  },
  "UserServiceBaseUrl": "http://localhost:8081",
  "AllowedCors": [ "http://localhost:4200" ],
  "ConnectionStrings": {
    "StorageAccount": "STORAGE_ACCOUNT_CONNECTION_STRING"
  },
  "Google": {
    "ClientId": "CLIENT_ID",
    "ClientSecret": "CLIENT_SECRET"
  }
}